{
  "name": "music-bot",
  "version": "0.0.1",
  "description": "Open source music.",
  "main": "dist/index.js",
  "scripts": {
    "start": "concurrently \"npm:Boat\" \"npm:LavaServer\"  ",
    "prestart": "tsc -b --verbose",
    "Boat": "node dist/index.js",
    "LavaServer": "java -jar ./Lavalink.jar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pixel1011/musicboat.git"
  },
  "keywords": [
    "Discord",
    "Music",
    "Youtube",
    "Twitch",
    "Soundcloud",
    "Spotify"
  ],
  "author": "Pixel",
  "license": "",
  "bugs": {
    "url": "https://github.com/Pixel1011/musicboat/issues"
  },
  "homepage": "https://github.com/Pixel1011/musicboat#readme",
  "dependencies": {
    "@lavaclient/spotify": "^3.1.0",
    "cheerio": "^1.0.0-rc.12",
    "concurrently": "^7.5.0",
    "discord.js": "^14.7.0",
    "lavaclient": "^4.1.1",
    "minimist": "^1.2.6",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@lavaclient/types": "^2.1.1",
    "@types/dateformat": "^5.0.0",
    "@types/minimist": "^1.2.2",
    "@types/node-fetch": "^2.6.2",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "eslint": "^8.27.0"
  }
}
